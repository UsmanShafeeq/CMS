{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "env": {
    "VITE_API_URL": "@vite_api_url"
  },
  "routes": [
    {
      "src": "^/(?!api).*",
      "dest": "/index.html",
      "status": 200
    }
  ],
  "ignoreCommand": "git check-ignore -q $FILE_PATH"
}
